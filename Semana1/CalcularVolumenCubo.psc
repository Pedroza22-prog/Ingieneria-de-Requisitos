Proceso CalcularVolumenCubo
	Definir lado, volumen Como Real;
	// Leer la longitud del lado del cubo
	Escribir 'Ingrese la longitud del lado del cubo (en cm): ';
	Leer lado;
	// Calcular el volumen del cubo
	volumen <- lado*lado*lado;
	// Mostrar el volumen del cubo
	Escribir 'El volumen del cubo es: ', volumen, ' cm3';
FinProceso
